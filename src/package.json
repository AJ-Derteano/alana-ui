{
  "name": "alana-ui",
  "description": "Una biblioteca de componentes UI para React Native. Ofrece una amplia gama de componentes personalizables para crear interfaces elegantes y funcionales en aplicaciones móviles. Documentación completa y ejemplos de uso disponibles. Ideal para desarrolladores que buscan coherencia visual y usabilidad.",
  "version": "0.0.1",
  "private": false,
  "main": "expo/AppEntry.js",
  "files": [
    "src/",
    "dist/*",
    "types/*"
  ],
  "scripts": {
    "prepare": "bob build",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "targets": [
    [
      "aar",
      {
        "reverseJetify": true
      }
    ],
    [
      "commonjs",
      {
        "copyFlow": true
      }
    ],
    "module",
    "typescript"
  ],
  "dependencies": {
    "expo": "~51.0.2",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AJ-Derteano/alana-ui"
  },
  "author": "Andrés Derteano <andresderteano@outlook.com> (https://github.com/AJ-Derteano)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AJ-Derteano/alana-ui/issues"
  },
  "homepage": "https://github.com/AJ-Derteano/alana-ui#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}