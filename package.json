{
  "name": "alana-ui",
  "version": "0.0.1",
  "private": false,
  "main": "expo/AppEntry.js",
  "files": [
    "src/*",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "dist/*",
    "types/*"
  ],
  "scripts": {
    "prepare": "bob build",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "targets": [
    [
      "aar",
      {
        "reverseJetify": true
      }
    ],
    [
      "commonjs",
      {
        "copyFlow": true
      }
    ],
    "module",
    "typescript"
  ],
  "dependencies": {
    "expo": "~51.0.2",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-safe-area-context": "4.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^5.1.3"
  },
  "react-native": "src\\index.ts",
  "source": "src\\index.ts",
  "module": "lib\\module\\index.js",
  "types": "lib\\typescript\\src\\index.d.ts",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}